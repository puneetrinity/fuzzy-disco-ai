{
  "name": "fuzzy-disco-ai",
  "version": "1.0.0",
  "description": "AI-powered development tools and MCP server for Railway deployment",
  "type": "module",
  "main": "mcp-server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node mcp-server-global.js",
    "start:web": "node server.js",
    "dev": "nodemon mcp-server-global.js",
    "dev:web": "nodemon server.js",
    "build": "echo 'No build needed - pure JavaScript'",
    "test": "echo 'Tests would go here'",
    "lint": "echo 'Linting skipped'"
  },
  "keywords": [
    "ai",
    "mcp",
    "development-tools",
    "railway",
    "typescript",
    "workflow-automation"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/puneetrinity/fuzzy-disco-ai.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "files": [
    "dist/**/*",
    "mcp-server.js",
    "README.md",
    "LICENSE"
  ],
  "nodemonConfig": {
    "watch": ["dist"],
    "ext": "js",
    "ignore": ["node_modules/**/*"],
    "exec": "node dist/index.js"
  }
}